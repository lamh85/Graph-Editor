{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": false
      }
    ]
  ],
  "env": {
      "test": {
        "plugins": ["@babel/plugin-transform-runtime"]
      }
  }
}
